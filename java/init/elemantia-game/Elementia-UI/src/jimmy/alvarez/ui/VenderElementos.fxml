            <Image url="@../assets/main/fontbolt-removebg-preview.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="72.0" fitWidth="479.0" layoutX="239.0" layoutY="136.0" pickOnBounds="true" preserveRatio="true">
