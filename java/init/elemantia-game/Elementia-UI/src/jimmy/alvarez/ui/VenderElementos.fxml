<AnchorPane prefHeight="700.0" prefWidth="900.0" styleClass="create-tierra" stylesheets="@app.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jimmy.alvarez.tl.VenderElementos">
   <children>
      <ImageView fitHeight="21.0" fitWidth="166.0" layoutX="29.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true" styleClass="title-img">
